<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <container name="contact.top" htmlTag="div" htmlClass="contact-top" before="-">
                <block class="Magento\Framework\View\Element\Template" name="info.right" template="Magento_Contact::left.phtml"></block>
                <block class="Magento\Framework\View\Element\Template" name="map.right" after="-" template="Magento_Contact::right.phtml"> </block>   
            </container>
        </referenceContainer>
        <referenceBlock name="page.main.title" remove="true"/>
    </body>
</page>
