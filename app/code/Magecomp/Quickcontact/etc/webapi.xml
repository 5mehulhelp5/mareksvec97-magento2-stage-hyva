<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route method="GET" url="/V1/quickcontact/config">
        <service class="Magecomp\Quickcontact\Api\QuickContactInterface" method="getConfigData"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route method="POST" url="/V1/quickcontact/submitform">
        <service class="Magecomp\Quickcontact\Api\QuickContactInterface" method="setFormData"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
</routes>
